<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">

    <item>
        <shape>
            <corners android:radius="@dimen/_35sdp" />
            <solid android:color="@color/white" />
            <stroke android:width="@dimen/_1sdp" android:color="@color/black" tools:ignore="StateListReachable" />
        </shape>
    </item>
    <item android:state_selected="true" tools:ignore="StateListReachable">
